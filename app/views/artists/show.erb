<h1>Artist Details</h1>

<h2>Artist Name</h2>
<h3><%= @artist.name %></h3>

<h2>Artist Songs</h2>
<% @artist.songs.each do |song| %>
<a href='/songs/<%= song.slug %>'><%= song.name %></a><br>

<% end %>

<h2>Artist Genres</h2>
<% @artist.genres.each do |genre| %>
<a href='/genres/<%= genre.slug %>'><%= genre.name %></a><br>

<% end %>